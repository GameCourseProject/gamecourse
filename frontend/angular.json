{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gamecourse": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        },
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "routing": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gamecourse",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/@simonwep/pickr/dist/themes/monolith.min.css",
              "node_modules/nestable2/dist/jquery.nestable.min.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.min.css",
              "node_modules/slim-select/dist/slimselect.min.css",
              "node_modules/swagger-ui/dist/swagger-ui.css",
              "src/styles/main.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/@simonwep/pickr/dist/pickr.min.js",
              "node_modules/cronstrue/dist/cronstrue.min.js",
              "node_modules/nestable2/dist/jquery.nestable.min.js",
              "node_modules/apexcharts/dist/apexcharts.min.js",
              "node_modules/quill/dist/quill.min.js",
              "node_modules/quill-image-resize/image-resize.min.js",
              "node_modules/datatables.net/js/jquery.dataTables.min.js",
              "node_modules/slim-select/dist/slimselect.min.js",
              "node_modules/highcharts/highcharts.js",
              "node_modules/highcharts/modules/sankey.js",
              "node_modules/highcharts/modules/exporting.js",
              "node_modules/highcharts/modules/export-data.js",
              "node_modules/highcharts/modules/accessibility.js"
            ],
            "allowedCommonJsDependencies": [
              "@braintree/sanitize-url",
              "@simonwep/pickr",
              "apexcharts",
              "base64-js",
              "classnames",
              "codemirror",
              "cronstrue",
              "core-js-pure",
              "css.escape",
              "deepmerge",
              "hoist-non-react-statics",
              "html2canvas",
              "lodash",
              "moment",
              "prop-types",
              "quill",
              "quill-image-resize",
              "randexp",
              "react-copy-to-clipboard",
              "react-debounce-input",
              "react-immutable-proptypes",
              "redux-immutable",
              "traverse",
              "url-parse",
              "xml-but-prettier"
            ]
          },
          "configurations": {
            "production": {
              "baseHref": "/gamecourse/",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "aot": true,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "gamecourse:build:production"
            },
            "development": {
              "browserTarget": "gamecourse:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gamecourse:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles/main.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
